    "            bsum = bsum / len(arr)\n",
    "            rgb.append(rsum /255)\n",
    "            rgb.append(gsum /255)\n",
    "            rgb.append(bsum /255)\n",
    "            Cmulti.append(rgb)\n",
    "            Ymulti.append(len(temp))\n",
    "    g_arr = np.delete(g_arr,0)"
